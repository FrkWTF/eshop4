<dl id = 'liquors'>
  <% for liquor in @liquors %>
    <dt>
      <%= link_to liquor.name, :action => 'show', :id => liquor %>
      <%= link_to '+', :controller => 'cart', :action => 'add', :id => liquor, :remote => true %>
    </dt>
    <% for supplier in liquor.suppliers %>
      <dd><%= supplier.last_name %>, <%= supplier.first_name %></dd>
    <% end %>
    <dd><%= sprintf("Precio: %.2f â‚¬", liquor.price) %> </dd>
    <dd><small>Productor: <%= liquor.producer.name %></small></dd>
  <% end %>
</dl>
